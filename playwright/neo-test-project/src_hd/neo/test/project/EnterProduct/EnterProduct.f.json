{
  "$schema" : "https://json-schema.axonivy.com/14.0-dev/project/form.json",
  "components" : [ {
    "cid" : "input1",
    "type" : "Input",
    "config" : {
      "required" : "false",
      "label" : "Product",
      "value" : "#{data.data.product}"
    }
  }, {
    "cid" : "input2",
    "type" : "Input",
    "config" : {
      "required" : "false",
      "label" : "Release Date",
      "value" : "#{data.data.releaseDate}"
    }
  }, {
    "cid" : "input3",
    "type" : "Input",
    "config" : {
      "required" : "false",
      "label" : "Price",
      "value" : "#{data.data.price}"
    }
  }, {
    "cid" : "composite6",
    "type" : "Composite",
    "config" : {
      "name" : "neo.test.project.FormComposite",
      "startMethod" : "start",
      "parameters" : {
        "data" : "#{data.data}"
      }
    }
  }, {
    "cid" : "layout4",
    "type" : "Layout",
    "config" : {
      "components" : [ {
        "cid" : "button5",
        "type" : "Button",
        "config" : {
          "name" : "Proceed",
          "action" : "#{logic.close}"
        }
      } ],
      "type" : "FLEX",
      "justifyContent" : "END"
    }
  } ]
}